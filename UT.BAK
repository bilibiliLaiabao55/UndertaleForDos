	frisk_left,
	frisk_up
};
void draw(const int* pixelData, int width, int height, int x, int y, int flipX) {
    int y1, x1;
    for (x1 = 0; x1 < width; ++x1) {
		for (y1 = 0; y1 < height; ++y1) {
			temp0 = pixelData[y1*width+x1];
			if(flipX==1){temp0 = pixelData[y1*width+(width-1-x1)];}
			if(temp0==255){
				continue;
			}
			putpixel(x1+x, y1+y, temp0);
		}
    }
}

void play_anim(){
	if(player_frame_delay<=0){
		if(player_frame<3)
			++player_frame;
		else
			player_frame=0;
		player_frame_delay=120;
	}else
	--player_frame_delay;
}
void camera_scroll(){
	temp0 = player_x-((getmaxx()-20)/2);
	if(temp0>0)camera_pos[0] = temp0;
	temp0 = player_y-((getmaxy()-30)/2);
	if(temp0>0)camera_pos[1] = temp0;
	setviewport(camera_pos[0], camera_pos[1], getmaxx(), getmaxy(), 0);
}
int gdriver = VGA;
int gmode = VGAMED;
void main(void) {
	registerbgidriver(EGAVGA_driver);
    initgraph(&gdriver, &gmode, "");
	camera_scroll(0, 0);
    setbkcolor(EGA_MAGENTA);
    while(!done){
		delay(FPS);
		camera_scroll();
		flushall();
		draw(frisk_images[player_face][player_frame],20,30,player_x,player_y, (player_face==2));
		temp0 = inportb(0x60);
		if(temp0 != 0) key_input = temp0;
		if(key_input==1){
			done=1;
		}
		if(key_input==72){
			play_anim();
			player_face=3;
			player_y-=0.1;
		}
		if(key_input==80){
			play_anim();
			player_face=0;
			player_y+=0.1;
		}
		if(key_input==75){
			play_anim();
			player_face=1;
			player_x-=0.1;
		}
		if(key_input==77){
			play_anim();
			player_face=2;
			player_x+=0.1;
		}
	}
	clrscr();
    closegraph();
}